<?xml version="1.0"?>
<database xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="zed"
xsi:noNamespaceSchemaLocation="http://static.spryker.com/schema-01.xsd"
namespace="Orm\Zed\Ant\Persistence" package="src.Orm.Zed.Ant.Persistence">    

	<table name="pyz_ant" idMethod="native" allowPkInsert="true" phpName="PyzAnt">
		<column name="id_ant" required="true" type="INTEGER" primaryKey="true" autoIncrement="true"/>        
		<column name="name" required="true" type="VARCHAR" size="255"/>        
		<unique name="pyz_ant-name">            
			<unique-column name="name"/>        
			</unique>        
			<id-method-parameter value="pyz_ant_pk_seq"/>    
			</table>
			
</database>
